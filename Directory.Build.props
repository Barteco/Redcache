<Project>
  
  <PropertyGroup Condition="'$(ProjectName)'!='Redcache.Tests'">
    <LangVersion>latest</LangVersion>
    <Version>0.0.1</Version>
    <TreatWarningsAsErrors>true</TreatWarningsAsErrors>
    <Authors>Barteco</Authors>
    <Copyright>Copyright Â© Barteco</Copyright>
    <PackageLicenseExpression>MIT</PackageLicenseExpression>
    <PackageRequireLicenseAcceptance>false</PackageRequireLicenseAcceptance>
    <RepositoryType>git</RepositoryType>
    <RepositoryUrl>https://github.com/Barteco/Redcache</RepositoryUrl>
    <PackageProjectUrl>https://github.com/Barteco/Redcache</PackageProjectUrl>
    <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
    <OutputType>Library</OutputType>
    <PackageOutputPath>$(SolutionDir)/artifacts</PackageOutputPath>
    <PackageIcon>logo.png</PackageIcon>
  </PropertyGroup>

  <ItemGroup Condition="'$(ProjectName)'!='Redcache.Tests'">
    <None Include="$(SolutionDir)/assets/logo.png" Pack="true" PackagePath="" />
  </ItemGroup>
  
  <ItemGroup Condition="'$(ProjectName)'!='Redcache.Tests'">
    <AssemblyAttribute Include="System.Runtime.CompilerServices.InternalsVisibleTo">
      <_Parameter1>Redcache.Tests</_Parameter1>
    </AssemblyAttribute>
  </ItemGroup>
  
</Project>